{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions" : "off" , 
        "no-console" : "off",
        "class-methods-use-this" : "off", 
        "new-cap" : "off" , 
        "consistent-return" : "off"
    }
}
